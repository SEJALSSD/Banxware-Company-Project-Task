    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "build",
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "vitest/globals"
    ]
  }
}
